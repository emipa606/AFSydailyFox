<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- Left -->
  <HediffDef ParentName="SydailyOnCheekBase">
    <defName>SydailyOnCheek_Left</defName>
    <label>斯黛莉水晶【左】</label>
    <description>这是斯黛莉晶狐左脸脸颊上的水晶，
		\n\n摸上去冰冰凉，希望狐狸不会感冒~</description>
  </HediffDef>
  <HediffGiverSetDef>
    <defName>SydailyFoxHediffSet_SydailyOnCheek_Left</defName>
    <hediffGivers>
      <li Class="HediffGiver_Birthday">
        <hediff>SydailyOnCheek_Left</hediff>
        <ageFractionChanceCurve>
          <points>
            <li>(0, 1)</li>
            <li>(1, 1)</li>
          </points>
        </ageFractionChanceCurve>
      </li>
    </hediffGivers>
  </HediffGiverSetDef>
  <!-- Right -->
  <HediffDef ParentName="SydailyOnCheekBase">
    <defName>SydailyOnCheek_Right</defName>
    <label>斯黛莉水晶【右】</label>
    <description>这是斯黛莉晶狐右脸脸颊上的水晶，
		\n\n摸上去也是冰冰凉的。</description>
  </HediffDef>
  <HediffGiverSetDef>
    <defName>SydailyFoxHediffSet_SydailyOnCheek_Right</defName>
    <hediffGivers>
      <li Class="HediffGiver_Birthday">
        <hediff>SydailyOnCheek_Right</hediff>
        <ageFractionChanceCurve>
          <points>
            <li>(0, 1)</li>
            <li>(1, 1)</li>
          </points>
        </ageFractionChanceCurve>
      </li>
    </hediffGivers>
  </HediffGiverSetDef>
</Defs>
